using System;
public class Test
{
	public static void Main()
	{  
	   int N = Convert.ToInt32(Console.ReadLine());
	   int nearest = 1;
	   
	   while (nearest < N){
	       nearest*=2;
	   }	   
	   nearest/=2;	   
	   int ost = N - nearest;
	   Console.WriteLine(Math.Min(ost, nearest - ost));
	}
}
